.address {
    height: 520rpx;
    font-size: 28rpx;
    overflow: hidden;
}

.address .addAddress .selWrap {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    margin: 20rpx 30rpx;
}

.address .addAddress .selWrap .selName {
    width: 160rpx;
    text-align: right;
    margin: 20rpx 0;
}

.address .addAddress .selWrap .ipt {
    width: 550rpx;
    margin-left: 30rpx;
}

.setDefault {
    display: flex;
    justify-content: space-between;
    margin: 40rpx 20rpx 20rpx 20rpx;
}

.addAdd {
    width: 340rpx;
    height: 80rpx;
    color: #ffffff;
    border-radius: 12rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
    background-color: #e7691d;
}

.line {
    border: 1rpx solid #e4e1e1;
}