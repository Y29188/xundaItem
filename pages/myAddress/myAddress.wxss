page {
    background-color: #ffffff;
}

.myAddress {
    margin: 30rpx 20rpx;
    /* font-size: 28rpx; */
    overflow: hidden;
}

.info {
    display: flex;
}

.username {
    margin-left: 30rpx;
}

.avatar {
    width: 64rpx;
    height: 64rpx;
    line-height: 64rpx;
    background-color: #ffa726;
    color: rgba(255, 255, 255, 100);
    font-size: 24rpx;
    text-align: center;
    border: 1px solid #ffa726;
    border-radius: 100%;
}



.del_address {
    margin: 20rpx 180rpx;
}

.flex {
    display: flex;
}

.value {
    margin-left: 30rpx;

}

.btn {
    width: 200px;
    height: 40px;
    font-size: 34rpx;
    background-color: #e7691d;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #ffffff;
    /* margin: 0rpx 20rpx; */
    border-radius: 12rpx;
    margin-top: 100rpx;
    margin-left: 180rpx;
}

.morenAddress {
    display: flex;
    margin-top: 20rpx;
    font-size: 28rpx;
    justify-content: space-between;
}

.address {
    font-size: 28rpx;
    margin-top: 10rpx;
    color: rgba(153, 153, 153, 100);
}